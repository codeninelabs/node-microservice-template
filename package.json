{
  "name": "nodemicroservice",
  "version": "0.0.0",
  "description": "Node MicroService Template",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc --skipLibCheck",
    "start": "npx tsc --watch --skipLibCheck && node ./dist/server.js",
    "dev:tsc": "tsc --watch -p .",
    "dev:serve": "nodemon -e js -w lib dist/server.js",
    "dev:old": "run-p dev:*",
    "dev": "nodemon"
  },
  "keywords": [],
  "author": "Kevin Armstrong",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.1.0",
    "@grpc/proto-loader": "^0.5.0",
    "async": "^1.5.2",
    "dotenv": "^10.0.0",
    "google-protobuf": "^3.19.1",
    "grpc": "^1.24.11",
    "lodash": "^4.17.21",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types-first-api/grpc-common": "^1.0.2-alpha.7",
    "@types-first-api/grpc-server": "^1.0.2-alpha.7",
    "@types/dayjs-precise-range": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/google-protobuf": "^3.15.5",
    "@types/grpc-error": "^1.0.0",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.6",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
